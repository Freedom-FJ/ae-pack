{
  "name": "@ali/aepay-ui",
  "version": "1.0.2",
  "title": "aepay B端基础组件库",
  "description": "aepay B端基础组件库",
  "files": [
    "esm",
    "es2017",
    "cjs",
    "dist",
    "build"
  ],
  "main": "esm/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "exports": {
    ".": {
      "es2017": {
        "types": "./es2017/index.d.ts",
        "default": "./es2017/index.js"
      },
      "default": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      }
    },
    "./*": "./*"
  },
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.less",
    "*.css"
  ],
  "scripts": {
    "dev": "ice-pkg start",
    "start": "ice-pkg start",
    "build": "ice-pkg build",
    "clean": "rm -rf node_modules/* && rm -rf .docusaurus && rm -rf .pegasus  && rm -rf esm && rm -rf es2017 && rm -rf dist && rm -rf build",
    "prepublishOnly": "npm run build",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "lint": "npm run eslint && npm run stylelint"
  },
  "keywords": [
    "ice",
    "react",
    "component"
  ],
  "dependencies": {
    "@ali/aepay-hooks": "*",
    "@ali/aepay-utils": "*",
    "@ali/fc-design-card-logo": "^0.1.2",
    "@alifd/next": "^1.25.51",
    "@alife/theme-csp-seller": "^0.28.1",
    "@ant-design/icons": "^5.2.6",
    "@ice/jsx-runtime": "^0.2.0",
    "@swc/helpers": "^0.4.14",
    "libphonenumber-js": "^1.10.53",
    "rc-image": "^7.10.0"
  },
  "devDependencies": {
    "@applint/spec": "^1.2.3",
    "@ice/pkg": "^1.5.5",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stylelint": "^15.0.0"
  },
  "componentConfig": {
    "name": "aepay-ui",
    "title": "aepay-ui",
    "category": ""
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18"
  }
}